<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.temanbisindo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_aplikasi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_aplikasi"
        android:supportsRtl="true"
        android:theme="@style/Theme.TemanBisindo">
        <activity android:name=".Common.OnBoarding"></activity>
        <activity android:name=".Common.KatatanyaActivity" />
        <activity android:name=".Common.BulanhariActivity" />
        <activity android:name=".Common.AngkaActivity" />
        <activity android:name=".Common.TentangActivity" />
        <activity android:name=".Common.SpeechtoTextActivity" />
        <activity android:name=".Common.HurufActivity" />
        <activity android:name=".Common.MainActivity" />
        <activity android:name=".Common.HomeActivity" />
        <activity android:name=".Common.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>